g   &lt;- tableGrob(t1,rows = NULL)

ind &lt;- map_dbl(1:7, ~find_cell(g, 6, .x, &quot;core-bg&quot;))  

walk(ind, function(i) g$grobs[i][[1]][[&quot;gp&quot;]] &lt;&lt;-
